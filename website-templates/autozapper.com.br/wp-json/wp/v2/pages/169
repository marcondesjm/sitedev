 => {\r\n            const nomesGerados = new Set();\r\n\r\n            while (nomesGerados.size < 150) {\r\n                const nome = nomesBase[Math.floor(Math.random() * nomesBase.length)];\r\n                const sobrenome = `${letrasSobrenomes[Math.floor(Math.random() * letrasSobrenomes.length)]}.`;\r\n                nomesGerados.add(`${nome} ${sobrenome}`);\r\n            }\r\n\r\n            return Array.from(nomesGerados);\r\n        };\r\n\r\n        const nomes = gerarNomesUnicos();\r\n        let indiceAtual = 0;\r\n\r\n        function mostrarNotificacao() {\r\n            if (indiceAtual >= nomes.length) indiceAtual = 0;\r\n\r\n            const compraAleatoria = {\r\n                nome: nomes[indiceAtual],\r\n                tempo: `${Math.floor(Math.random() * 60)} minutos`,\r\n                pagamento: Math.random() > 0.5 ? \"Pix\" : \"Cart\u00e3o de cr\u00e9dito\",\r\n            };\r\n\r\n            document.querySelector('#toast-compras .nome').innerText = compraAleatoria.nome;\r\n            document.querySelector('#toast-compras .tempo').innerText = `h\u00e1 ${compraAleatoria.tempo}`;\r\n            document.querySelector('#toast-compras .pagamento').innerText = `comprou no ${compraAleatoria.pagamento}`;\r\n\r\n            document.querySelector('#toast-compras').classList.add('show');\r\n            setTimeout(() => document.querySelector('#toast-compras').classList.remove('show'), 9000);\r\n\r\n            indiceAtual++;\r\n        }\r\n\r\n        \/\/ Iniciando a sequ\u00eancia das notifica\u00e7\u00f5es\r\n        setTimeout(() => {\r\n            mostrarNotificacao();\r\n            setInterval(mostrarNotificacao, intervaloNotificacoes);\r\n        }, delayInicial);\r\n    <\/script>\r\n\r\n    <style>\r\n        \/* Estiliza\u00e7\u00e3o do toast *\/\r\n        :root {\r\n            --cor-de-destaque: #f00;\r\n        }\r\n\r\n        #views b,\r\n        #vagas b {\r\n            color: var(--cor-de-destaque);\r\n        }\r\n\r